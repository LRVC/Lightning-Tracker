<div class= 'container'>
    <div class="page-header">
      <div class="text-center">
        <h1> Lightning Tracker </h1>
      </div>
    </div>

    <div class='row'>
        <div class= 'col-sm-offset-4 col-sm-4'>
          <div class= 'text-center'>
              <% if current_user %>
                <%= link_to "Sign Out", signout_path %>
              <% else %>
                <%= link_to "Sign Up", signup_path %>
                |
                <%= link_to "Sign In", signin_path %>
              <% end %>
          </div>
        </div>

        <div class= 'col-sm-4'>
        </div>

    </div>
</div
